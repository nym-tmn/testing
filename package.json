{
  "name": "testing",
  "homepage": "https://nym-tmn.github.io/testing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress:open:storybook": "cypress open --env storybook=true",
    "cypress:run:storybook": "cypress run --env storybook=true",
    "cypress:update:storybook": "cypress run --env updateSnapshots=true,storybook=true",
    "cypress:open:app": "cypress open --env storybook=false",
    "cypress:run:app": "cypress run --env storybook=false",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/js": "^9.25.0",
    "@simonsmith/cypress-image-snapshot": "^9.1.0",
    "@storybook/addon-docs": "^9.0.12",
    "@storybook/react-vite": "^9.0.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cypress": "^1.1.6",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "cypress": "^14.5.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.12",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "storybook": "^9.0.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.2"
  }
}
